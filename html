// Data SDK Handler
const dataHandler= {
    onDataChanged(data) {
        currentRecords=data;

        console.log(`Contact messages: $ {
                data.length
            }

            `);
    }
}

;

// Initialize Data SDK
async function initDataSdk() {
    if (window.dataSdk) {
        const result=await window.dataSdk.init(dataHandler);

        if ( !result.isOk) {
            console.error("Failed to initialize data SDK");
        }
    }
}

// Configuration update function
async function onConfigChange(config) {
    // Update chef name
    const chefName=config.chef_name || defaultConfig.chef_name;
    document.querySelector('h1').textContent=chefName;

    document.title=`$ {
        chefName
    }

    - Interactive Portfolio`;

    // Update title
    const title=config.title || defaultConfig.title;
    document.querySelector('header p').textContent=title;

    // Update contact information
    const phone=config.phone || defaultConfig.phone;
    const email=config.email || defaultConfig.email;
    const location=config.location || defaultConfig.location;

    // Update portfolio description
    const portfolioDesc=config.portfolio_description || defaultConfig.portfolio_description;
    const portfolioDescElement=document.querySelector('#about .text-xl.text-brand-accent');

    if (portfolioDescElement) {
        portfolioDescElement.textContent=portfolioDesc;
    }

    // Update colors
    const primaryColor=config.primary_color || defaultConfig.primary_color;
    const secondaryColor=config.secondary_color || defaultConfig.secondary_color;
    const accentColor=config.accent_color || defaultConfig.accent_color;
    const backgroundColor=config.background_color || defaultConfig.background_color;
    const textColor=config.text_color || defaultConfig.text_color;

    document.documentElement.style.setProperty('--primary-color', primaryColor);
    document.documentElement.style.setProperty('--secondary-color', secondaryColor);
    document.documentElement.style.setProperty('--accent-color', accentColor);
    document.documentElement.style.setProperty('--background-color', backgroundColor);
    document.documentElement.style.setProperty('--text-color', textColor);

    // Apply colors to elements
    document.body.style.background=`linear-gradient(135deg, $ {
            backgroundColor
        }

        0%, $ {
            backgroundColor
        }

        dd 100%)`;
    document.body.style.color=textColor;

    // Update primary color elements
    document.querySelectorAll('h1, h2, h3, .text-brand-primary').forEach(element=> {
            element.style.color=primaryColor;
        });

    // Update secondary color elements
    document.querySelectorAll('.text-brand-secondary').forEach(element=> {
            element.style.color=secondaryColor;
        });

    // Update accent color elements
    document.querySelectorAll('.text-brand-accent').forEach(element=> {
            element.style.color=accentColor;
        });
}

// Initialize SDK when available
